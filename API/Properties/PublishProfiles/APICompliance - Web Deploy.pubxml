<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ResourceId>/subscriptions/b762de0c-38d1-4275-bf81-4979b2d99248/resourcegroups/ComplianceGroup/providers/Microsoft.Web/sites/APICompliance</ResourceId>
    <ResourceGroup>ComplianceGroup</ResourceGroup>
    <PublishProvider>AzureWebSite</PublishProvider>
